{
    "source": "./src",
    "destination": "./docs",
    "includes": [
        "\\.js$"
    ],
    "excludes": [],
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "test": {
                    "source": "./__tests__"
                }
            }
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {
                "all": true
            }
        },
        {
            "name": "esdoc-jsx-plugin",
            "option": {
                "enable": true
            }
        },
        {
            "name": "esdoc-inject-style-plugin",
            "option": {
                "enable": true,
                "styles": ["./resources/readme.css"]
            }
        }
    ]
}